<div class="container">
<h1>Log In</h1>
<p id="alert"><%= alert %></p>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %><br>
  </div>
  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %><br>
  </div>
  <br>
  <div class="actions"><%= submit_tag "Log In" %></div>
<% end %>
</div>
